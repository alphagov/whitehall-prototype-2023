<hr style="margin-top: 0.5em;" />

<div class="nhsuk-grid-row">
  <div class="nhsuk-grid-column-two-thirds">
    <h2>
        <a href="/marketing-page/">{{params.solution}}</a>
    </h2>

    <h3>
      {{params.supplier}}
    </h3>

    <p>
      {{params.text}}
    </p>

    {% from 'checkboxes/macro.njk' import checkboxes %}

    {{ checkboxes({
      "idPrefix": "example",
      "name": "example",
      "items": [
        {
          "value": "compare",
          "text": "Compare"
        }
      ]
    }) }}
  </div>
  <div class="nhsuk-grid-column-one-third">
    <div class="category-list">
      <h3>Capabilities</h3>
      <ul>
      {% for item in params.capabilities %}
        <li>{{item.text}}</li>
      {% endfor %}
      </ul>
    </div>
  </div>
</div>
